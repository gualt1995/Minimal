{{#each icon_group}}
    <div class="icons_group">
        <div class="icon_header">
            <div class="icon_group_title text_display_small">{{title}}</div>
            <div class="icon_btn_stlye_frame">
                <svg width="20" height="20" viewBox="0 0 20 20"xmlns="http://www.w3.org/2000/svg" class="icon_selector_pill">
                    <circle cx="10" cy="10" r="10"/>
                </svg>
                <svg width="20" height="20" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg" class="icon_selector_pill">
                    <path d="M17 10C17 13.866 13.866 17 10 17C6.13401 17 3 13.866 3 10C3 6.13401 6.13401 3 10 3C13.866 3 17 6.13401 17 10Z"/>
                    <path fill-rule="evenodd" clip-rule="evenodd" d="M10 20C15.5228 20 20 15.5228 20 10C20 4.47715 15.5228 0 10 0C4.47715 0 0 4.47715 0 10C0 15.5228 4.47715 20 10 20ZM19 10C19 14.9706 14.9706 19 10 19C5.02944 19 1 14.9706 1 10C1 5.02944 5.02944 1 10 1C14.9706 1 19 5.02944 19 10Z"/>
                </svg>
            </div>
            <a class="icon_figma_link" href="{{figma_link}}">
                <span>Source in Figma</span>
                <div class="link_indicator">
                    <svg width="20" height="20" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7.43412 13.6281L12.6356 8.42664V12.9715C12.6356 13.3347 12.9336 13.6327 13.2968 13.6327C13.66 13.6327 13.9534 13.3394 13.9534 12.9762L13.9534 6.83874C13.9534 6.47552 13.66 6.18215 13.2968 6.18215L7.15938 6.17284C6.98519 6.17264 6.81807 6.24176 6.69489 6.36493C6.57172 6.48811 6.5026 6.65522 6.5028 6.82942C6.5028 7.19264 6.79617 7.486 7.15938 7.486L11.7042 7.49532L6.5028 12.6968C6.24669 12.9529 6.24669 13.372 6.5028 13.6281C6.75891 13.8842 7.17801 13.8842 7.43412 13.6281Z"/>
                    </svg>
                </div>
            </a>
        </div>
        <div class="icon_group_list" id="icons_group_monitoring">
            {{#each styles}}
                <div class="icon_style" id={{name}}>
                    {{#each icons}}
                        <div class="icon_display">
                            <svg id="{{icon_name}}"class="icons_svg" width="24" height="24" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                                <path fill-rule="evenodd" clip-rule="evenodd" d="{{icon_path}}"/>
                            </svg>
                            <span class="text_light"> {{icon_name}}</span>
                        </div>
                    {{/each}}
                </div>
            {{/each}}
        </div>
    </div>
{{/each}}
<div class="icons_preview_frame">
    <div class="icon_preview_container">
        <svg class="icons_preview_image" width="300" height="300" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd" clip-rule="evenodd" d="M3 2C2.44772 2 2 2.44772 2 3V20C2 21.1046 2.89543 22 4 22H21C21.5523 22 22 21.5523 22 21C22 20.4477 21.5523 20 21 20H5C4.44772 20 4 19.5523 4 19V3C4 2.44772 3.55228 2 3 2ZM18.7541 8.17138C19.5693 6.82764 20.3139 5.60033 21 5V18C21 18.5523 20.5523 19 20 19H6C5.44772 19 5 18.5523 5 18V14.2177C5 14.0743 5.03022 13.9323 5.09243 13.803C6.25854 11.3794 8.40028 9.21597 10.7143 12C14.0191 15.9761 16.627 11.6774 18.7541 8.17138Z"/>
        </svg>
        <span class="text_light"> Icon name</span>
    </div>
    <div class="Icon_preview_close_btn icon_btn">
        <svg width="24" height="24" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" alt="close" id="icon_btn_cv_close_img">
            <path d="M18.3 5.70999C18.1131 5.52274 17.8595 5.41751 17.595 5.41751C17.3305 5.41751 17.0768 5.52274 16.89 5.70999L12 10.59L7.10997 5.69999C6.92314 5.51274 6.66949 5.40751 6.40497 5.40751C6.14045 5.40751 5.8868 5.51274 5.69997 5.69999C5.30997 6.08999 5.30997 6.71999 5.69997 7.10999L10.59 12L5.69997 16.89C5.30997 17.28 5.30997 17.91 5.69997 18.3C6.08997 18.69 6.71997 18.69 7.10997 18.3L12 13.41L16.89 18.3C17.28 18.69 17.91 18.69 18.3 18.3C18.69 17.91 18.69 17.28 18.3 16.89L13.41 12L18.3 7.10999C18.68 6.72999 18.68 6.08999 18.3 5.70999Z"/>
        </svg>   
    </div>
</div>




                    
                       
